{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Run parser",
            "program": "${workspaceFolder}\\services\\parser\\tree2labels\\run_ncrfpp.py",
            "cwd": "${workspaceFolder}",
            "args": [
                "--test",
                "${workspaceFolder}\\services\\parser\\tmp\\intermediate_output.seq_lu",
                // "--gold",
                // ".\\test.txt",
                "--model",
                "${workspaceFolder}\\services\\parser\\tmp\\model\\ptb-bilstm2-chlstm-glove",
                "--status",
                "test",
                "--gpu",
                "False",
                "--output",
                "${workspaceFolder}\\services\\parser\\tmp\\output",
                "--evalb",
                ".\\EVALB\\evalb",
                "--ncrfpp",
                "${workspaceFolder}\\services\\parser\\tree2labels\\NCRFpp",
                 ]
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Train parser",
            "program": "${workspaceFolder}\\services\\parser\\train.py",
            "cwd": "${workspaceFolder}",
        },
        {
            "type": "python",
            "request": "launch",
            "name": "NLP service",
            "program": "${workspaceFolder}\\services\\nlp\\run_nlp_serivce.py",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Mapping Service",
            "program": "${workspaceFolder}\\services\\mapping\\run_mapping_service.py",
            "cwd": "${workspaceFolder}"
        }
    ],
    "compounds": [
        {
            "name": "Services",
            "configurations": [
                "NLP service",
                "Mapping Service"
            ]
        }
    ]
}